let numeroUm = 1
let stringUm = '1'
let numeroTrinta = 30
let stringTrinta = '30'
let numeroDez = 10
let stringDez = '10'

if (numeroUm == stringUm) {
    console.log ('1: As variáveis numeroUm e stringUm tem o mesmo valor, mas tipos diferentes')
} else {
    console.log ('1: As váriaveis numeroUm e stringUm não tem o mesmo valor')
}

if (numeroTrinta == stringTrinta) {
    console.log ('30: As várias numeroTrinta e stringTrinta tem o mesmo valor, mas tipos diferentes')
} else {
    console.log ('30: As várias numeroTrinta e stringTrinta não tem o mesmo valor')
}

if (numeroDez == stringDez) {
    console.log('10: As váriaveis numeroDez e stringDez tem o mesmo valor, mas tipos diferentes')
} else {
    console.log('10: As váriaveis numeroDez e stringDez não tem o mesmo valor')
}

